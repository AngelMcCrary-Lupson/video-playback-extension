{
  "manifest_version": 2,

  "name": "Playback Speed Controller",
  "description": "Adjust a video's playback speed.",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "icons": { "16": "icon.png",
           "48": "icon.png",
          "128": "icon.png" },
  "permissions": [
   "activeTab"
   ]
}
